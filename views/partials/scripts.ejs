<script src="/dist/sweetalert2.js"></script>
<script>
    <% if (typeof messages.success != 'undefined' && messages.success != '') { %>
            Swal.fire({
            text: "<%= messages.success %>",
            icon: 'success'
            })
    <% } else if (typeof messages.error != 'undefined' && messages.error != '') { %>
                Swal.fire({
                text: "<%= messages.error %>",
                icon: 'error'
        })
    <% } %>

    //change Email
    $("#emailBtn").click(function (event) {
            if ($('#emailVal').val() != $('#oldEmail').val()) {
                event.preventDefault();
                $('#msg').text(`This is not your current email`)
            } else if ($('#email1').val() != $('#email2').val()) {
                event.preventDefault();
                $('#msg').text('Email mismatch')
            } else {
            }
        });
    $("#passwordBtn").click(function (event) {
            if ($('#password1').val() != $('#password2').val()) {
                event.preventDefault();
                $('#msg').text('Password mismatch')
            } else {

            }
        });
</script>
